{
  "name": "abstract-form-builder",
  "version": "1.0.0",
  "description": "Abstract Form Builder Package",
  "scripts": {
    "dev": "npx @tailwindcss/cli -i ./resources/styles/input.css -o ./resources/styles/public/larafields.css --watch",
    "build:dev": "npx @tailwindcss/cli -i ./resources/styles/input.css -o ./resources/styles/public/larafields.css --sourcemap",
    "build:prod": "npx @tailwindcss/cli -i ./resources/styles/input.css -o ./resources/styles/public/larafields.css --minify",
    "build": "npm run build:prod",
    "watch": "npm run dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  }
}
